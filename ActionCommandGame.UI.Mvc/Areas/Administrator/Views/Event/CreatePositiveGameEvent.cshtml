@model ActionCommandGame.UI.Mvc.Areas.Administrator.Models.SavePositiveEventResource

<form method="post">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control">
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Description"></label>
        <input asp-for="Description" class="form-control">
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Money"></label>
        <input asp-for="Money" class="form-control" value="0">
        <span asp-validation-for="Money" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Experience"></label>
        <input asp-for="Experience" class="form-control" value="0">
        <span asp-validation-for="Experience" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Probability"></label>
        <input asp-for="Probability" class="form-control" value="0">
        <span asp-validation-for="Probability" class="text-danger"></span>
    </div>
    <input type="submit" class="btn btn-primary" value="Create item">
</form>