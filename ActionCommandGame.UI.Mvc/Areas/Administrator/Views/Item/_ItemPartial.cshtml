@model ActionCommandGame.Model.Item

<tr>
    <td>@Model.Id</td>
    <td>@Model.Name</td>
    <td>@Model.Description</td>
    <td>@Model.Price</td>
    <td>@Model.Fuel</td>
    <td>@Model.Attack</td>
    <td>@Model.Defense</td>
    <td>@Model.ActionCooldownSeconds</td>
    <td><a class="btn btn-primary" asp-area="Administrator" asp-controller="Item" asp-action="Update" asp-route-id="@Model.Id">Update</a></td>
    <td><a class="btn btn-danger" asp-area="Administrator" asp-controller="Item" asp-action="Delete" asp-route-id="@Model.Id">Remove</a></td>
</tr>
